<%= search_form_for [:admin, @search], class: "form-search row" do |f| %>
  <div class="col-sm-10">
    <%= f.search_field :email_or_fullname_cont, class: "form-control col-sm-10 right form_search", placeholder: t(".search_form") %>
  </div>
  <div class="col-sm-2">
    <%= f.submit t(".btn_search"), class: "btn btn-info search-submit" %>
  </div>
<% end %>
